#include<stdio.h>

void swap(double *&pa,double *&pb);
void swap(double *&pa,double *&pb)
{
	double temp;
	temp = *pa;
	*pa=*pb;
	*pb=temp;
}
int main()
{
	double a,b;
	
	printf("a = ");
	scanf("%f",&a);
	printf("b = ");
	scanf("%f",&b);
	
	swap(a,b);
	
	printf("a = %d \n b = %d",a,b); 
	
	return 0;
}

